local MarketplaceService = game:GetService("MarketplaceService")
local success, info = pcall(function()
    return MarketplaceService:GetProductInfo(game.PlaceId)
end)

local gameName = "Unknown Game"
if success and info and info.Name then
    gameName = info.Name
end

local Lighting = game:GetService("Lighting")
local originalClockTime = Lighting.ClockTime
local originalBrightness = Lighting.Brightness
local originalAmbient = Lighting.Ambient
local originalGlobalShadows = Lighting.GlobalShadows

local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()
local baboonTagPlaceId = 73908022013371

local Window = Rayfield:CreateWindow({
    Name = "Femboy Hub | " .. gameName,
    LoadingTitle = "Ricardo's Interface Suite",
    LoadingSubtitle = "Custom Scripts & Features",
    Theme = "Dark", -- Tema compatible con Delta
    ToggleUIKeybind = Enum.KeyCode.RightShift,
    DisableRayfieldPrompts = false,

    ConfigurationSaving = {
        Enabled = true,
        FolderName = "FemboyHubConfigs",
        FileName = gameName .. "_Config"
    },

    KeySystem = true,
    KeySettings = {
        Title = "Femboy Hub Access",
        Subtitle = "Restricted Entry",
        Note = "Complete Loot Labs Link to get your key (link Already Copied)",
        FileName = "FemboyHubKey",
        SaveKey = false,
        GrabKeyFromSite = false,
        Key = {"RicardoKey123"} -- ðŸ”‘ Tu key personalizada
    }
})

task.delay(0.5, function()
    Rayfield:Notify({
        Title = "System",
        Content = "Script Successfully Loaded âœ…",
        Duration = 4,
        Image = 4483362458
    })
end)

local LavaTab = Window:CreateTab("Lava", 4483362458)
local BaboonTab = Window:CreateTab("Baboon", 4483362458)
local VisualsTab = Window:CreateTab("Visuals", 4483362458)

LavaTab:CreateSection("Lava")
BaboonTab:CreateSection("Baboon")
VisualsTab:CreateSection("Visuals")

local function IsLava(player)
    return player.Team and player.Team.Name:lower():find("lava")
end

local function InfectAll()
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    local Character = LocalPlayer.Character
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then return end
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Team and player.Team.Name:lower():find("baboon") then
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
                task.wait(0.2)
            end
        end
    end
end

local function FollowSequential()
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Team and player.Team.Name:lower():find("baboon") then
            local Character = LocalPlayer.Character
            if Character and Character:FindFirstChild("HumanoidRootPart") and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
                repeat task.wait(0.5) until player.Team and player.Team.Name:lower():find("lava")
            end
        end
    end
    Rayfield:Notify({Title="Complete",Content="All Baboons converted to Lava",Duration=4})
end

local AutoFarmEnabled = false
LavaTab:CreateToggle({
    Name = "Auto Win Farm",
    CurrentValue = false,
    Flag = "AutoWinFarm",
    Callback = function(Value)
        AutoFarmEnabled = Value
        if AutoFarmEnabled then
            task.spawn(function()
                local Players = game:GetService("Players")
                local LocalPlayer = Players.LocalPlayer
                local wasLava = false
                while AutoFarmEnabled do
                    if game.PlaceId == baboonTagPlaceId then
                        if IsLava(LocalPlayer) then
                            if not wasLava then
                                Rayfield:Notify({Title="Status",Content="You are now Lava, infecting all Baboons",Duration=4})
                                wasLava = true
                            end
                            InfectAll()
                        else
                            for _, player in ipairs(Players:GetPlayers()) do
                                if IsLava(player) and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                                    LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
                                    Rayfield:Notify({Title="Teleport",Content="Teleported to Lava player: "..player.Name,Duration=3})
                                    break
                                end
                            end
                        end
                    end
                    task.wait(1)
                end
            end)
        end
    end
})

LavaTab:CreateButton({
    Name = "Follow Baboons â†’ Switch when Lava",
    Callback = function()
        if game.PlaceId == baboonTagPlaceId then
            if IsLava(game:GetService("Players").LocalPlayer) then
                FollowSequential()
            else
                Rayfield:Notify({Title="Team Check",Content="You are not in the Lava team",Duration=3})
            end
        else
            Rayfield:Notify({Title="Wrong Game",Content="This feature only works in Baboon Tag X",Duration=3})
        end
    end
})


local Players = game:GetService("Players")
local selectedTarget = nil
local HuntActive = false

local function GetPlayerNames()
    local names = {}
    for _,plr in ipairs(Players:GetPlayers()) do
        if plr ~= Players.LocalPlayer then
            table.insert(names, plr.Name)
        end
    end
    return names
end

local HuntDropdown = LavaTab:CreateDropdown({
    Name = "Select Player to Hunt",
    Options = GetPlayerNames(),
    CurrentOption = "",
    Flag = "HuntTarget",
    Callback = function(Option)
        if typeof(Option) == "table" then
            selectedTarget = Option[1]
        else
            selectedTarget = Option
        end
    end
})

LavaTab:CreateToggle({
    Name = "Hunt Selected Player",
    CurrentValue = false,
    Flag = "HuntToggle",
    Callback = function(Value)
        local lp = Players.LocalPlayer

        if Value then
           
            if not IsLava(lp) then
                Rayfield:Notify({
                    Title = "Hunt",
                    Content = "You must be Lava to hunt!",
                    Duration = 3
                })
                return
            end

            if not selectedTarget or selectedTarget == "" then
                Rayfield:Notify({
                    Title = "Hunt",
                    Content = "No player selected!",
                    Duration = 3
                })
                return
            end

            local target = Players:FindFirstChild(selectedTarget)
            if not target then
                Rayfield:Notify({
                    Title = "Hunt",
                    Content = "Target not found in server!",
                    Duration = 3
                })
                return
            end

            HuntActive = true
            Rayfield:Notify({
                Title = "Hunt",
                Content = "Now hunting "..target.Name.." until infected!",
                Duration = 4
            })

            task.spawn(function()
                while HuntActive and IsLava(lp) and target and target.Team and target.Team.Name:lower():find("baboon") do
                    if target.Character and target.Character:FindFirstChild("HumanoidRootPart") and lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
                        lp.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
                    end
                    task.wait(0.3)
                end
                HuntActive = false
                Rayfield:Notify({
                    Title = "Hunt",
                    Content = target.Name.." is now Lava or hunt stopped.",
                    Duration = 4
                })
            end)
        else
            HuntActive = false
            Rayfield:Notify({
                Title = "Hunt",
                Content = "Hunt stopped.",
                Duration = 3
            })
        end
    end
})

BaboonTab:CreateSlider({
    Name = "WalkSpeed Modifier",
    Range = {16,100},
    Increment = 1,
    CurrentValue = 16,
    Flag = "WalkSpeed",
    Callback = function(Value)
        local lp = game:GetService("Players").LocalPlayer
        if lp.Character and lp.Character:FindFirstChildOfClass("Humanoid") then
            lp.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = Value
        end
    end
})

BaboonTab:CreateButton({
    Name = "Create AFK Win Platform",
    Callback = function()
        local lp = game:GetService("Players").LocalPlayer
        local Character = lp.Character
        if Character and Character:FindFirstChild("HumanoidRootPart") then
            local platform = Instance.new("Part")
            platform.Size = Vector3.new(100,5,100)
            platform.Position = Character.HumanoidRootPart.Position + Vector3.new(0,1200,0)
            platform.Anchored = true
            platform.Transparency = 1
            platform.CanCollide = true
            platform.Parent = workspace
            Character.HumanoidRootPart.CFrame = platform.CFrame + Vector3.new(0,5,0)
            Rayfield:Notify({Title="AFK Platform",Content="Invisible platform created 1200 studs above the map",Duration=5})
        end
    end
})


local InfiniteJumpEnabled = false
BaboonTab:CreateToggle({
    Name = "Infinite Jump (Low)",
    CurrentValue = false,
    Flag = "InfiniteJump",
    Callback = function(Value)
        InfiniteJumpEnabled = Value
    end
})

game:GetService("UserInputService").JumpRequest:Connect(function()
    if InfiniteJumpEnabled then
        local lp = game:GetService("Players").LocalPlayer
        if lp.Character and lp.Character:FindFirstChildOfClass("Humanoid") then
            lp.Character:FindFirstChildOfClass("Humanoid").JumpPower = 30
            lp.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end)


-- FlyGuiV3 desde GitHub (BaboonTab)
BaboonTab:CreateButton({
    Name = "Activate FlyGuiV3",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
        Rayfield:Notify({
            Title = "Fly",
            Content = "FlyGuiV3 activated from GitHub",
            Duration = 3
        })
    end
})

local ESPEnabled, ShowChams = false,false

local function AddChams(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local hrp = player.Character.HumanoidRootPart
        if ShowChams then
            if not hrp:FindFirstChild("Cham") then
                local box = Instance.new("BoxHandleAdornment")
                box.Name = "Cham"
                box.Adornee = hrp
                box.Size = hrp.Size + Vector3.new(2,2,2)
                box.Color3 = Color3.new(1,0,0) -- rojo
                box.Transparency = 0.5
                box.ZIndex = 0
                box.AlwaysOnTop = true -- se ve a travÃ©s de paredes
                box.Parent = hrp
            end
        else
            if hrp:FindFirstChild("Cham") then
                hrp.Cham:Destroy()
            end
        end
    end
end

local function UpdateESP()
    for _,player in ipairs(game:GetService("Players"):GetPlayers()) do
        if player ~= game:GetService("Players").LocalPlayer then
            if IsLava(player) then
                if ESPEnabled then
                    AddChams(player)
                else
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        local hrp = player.Character.HumanoidRootPart
                        if hrp:FindFirstChild("Cham") then hrp.Cham:Destroy() end
                    end
                end
            else
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local hrp = player.Character.HumanoidRootPart
                    if hrp:FindFirstChild("Cham") then hrp.Cham:Destroy() end
                end
            end
        end
    end
end


VisualsTab:CreateToggle({
    Name = "Enable ESP (Lava)",
    CurrentValue = false,
    Flag = "ESPEnabled",
    Callback = function(Value)
        ESPEnabled = Value
        UpdateESP()
    end
})

VisualsTab:CreateToggle({
    Name = "Chams (Through Walls)",
    CurrentValue = false,
    Flag = "ShowChams",
    Callback = function(Value)
        ShowChams = Value
        UpdateESP()
    end
})


game:GetService("RunService").RenderStepped:Connect(function()
    if ESPEnabled then
        UpdateESP()
    end
end)
